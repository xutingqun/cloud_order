package com.xmsmart.xiuba.bean;

public class ChangePersonalBean extends BaseBean {

	private ChangePersonalDataBean data = new ChangePersonalDataBean();
	private String success;
	private String message = "";// 错误信息

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public ChangePersonalDataBean getData() {
		return data;
	}

	public void setData(ChangePersonalDataBean data) {
		this.data = data;
	}

	public String getSuccess() {
		return success;
	}

	public void setSuccess(String success) {
		this.success = success;
	}

	@Override
	public String toString() {
		return "LoginBean [data=" + data + ", success=" + success + "]";
	}

	public class ChangePersonalDataBean {
		private String id = "";// :头像Id
		private String rootPath = "";// :根路径
		private String filePath = "";// :头像路径
		private String fileName = "";// :头像名
		public String getId() {
			return id;
		}
		public void setId(String id) {
			this.id = id;
		}
		public String getRootPath() {
			return rootPath;
		}
		public void setRootPath(String rootPath) {
			this.rootPath = rootPath;
		}
		public String getFilePath() {
			return filePath;
		}
		public void setFilePath(String filePath) {
			this.filePath = filePath;
		}
		public String getFileName() {
			return fileName;
		}
		public void setFileName(String fileName) {
			this.fileName = fileName;
		}
		
	}
}
