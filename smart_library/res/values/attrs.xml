<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        
    </declare-styleable>
      <attr name="attrIconCall" format="reference" />
    <attr name="CCPactionMenuTextAppearance" format="reference" />
    <!-- Color for text that appears within action menu items. -->
    <attr name="CCPactionMenuTextColor" format="color|reference" />
	 <declare-styleable name="MaskLayout">
        <attr name="foreground" format="reference" />
        <attr name="content_margin" format="dimension" />
        <attr name="content_marginLeft" format="dimension" />
        <attr name="content_marginRight" format="dimension" />
        <attr name="content_marginTop" format="dimension" />
        <attr name="content_marginBottom" format="dimension" />
    </declare-styleable>
        <declare-styleable name="AppPanel">
        <attr name="dot_count" format="dimension" />
        <attr name="dot_selected" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="form_input">
        <attr name="form_layout" format="reference" />
        <attr name="form_title" format="string" />
        <attr name="form_hint" format="string" />
    </declare-styleable>
    <attr name="attrBgSelectedLetter" format="reference" />
    
    <declare-styleable name="setting_info">
        <attr name="item_titleText" format="string" />
        <attr name="item_detailText" format="string" />
        <attr name="item_accessoryType">
            <enum name="none" value="0" />
            <enum name="arrow" value="1" />
            <enum name="switchbutton" value="2" />
            <enum name="radio" value="3" />
        </attr>
        <attr name="item_showDivider" format="boolean" />
    </declare-styleable>
 <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>
    
    
    

    <declare-styleable name="Emojicon">
        <attr name="emojiconSize" format="dimension"/>
        <attr name="emojiconTextStart" format="integer"/>
        <attr name="emojiconTextLength" format="integer"/>
        <attr name="emojiconUseSystemDefault" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension"/>
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference"/>
        <attr name="shadow_right" format="reference"/>
        <attr name="shadow_bottom" format="reference"/>
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference"/>
    <declare-styleable name="call_control">
        <attr name="call_direct">
            <enum name="incoming" value="0" />
            <enum name="outgoing" value="1" />
            <enum name="inCall" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="SpinnerView">
        <attr name="Spinner_item" format="reference" />
        <attr name="Spinner_DropDownTitle" format="reference" />
    </declare-styleable>    
    
      <!-- Loading动画 -->
   <declare-styleable name="LoadingView">
	<attr name="loadingText" format="string"/>
        <attr name="loadingTextAppearance" format="reference"/>
    </declare-styleable>
     <!-- Loading圆形小球 -->
     <declare-styleable name="monindicator">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleSpacing" format="dimension" />
        <attr name="cycle" format="integer" />
    </declare-styleable>
</resources>
